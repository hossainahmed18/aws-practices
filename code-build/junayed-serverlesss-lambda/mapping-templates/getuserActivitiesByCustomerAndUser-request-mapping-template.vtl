{
    "version": "2017-02-28",
    "operation" : "Query",
    "query" : {
        ## Provide a query expression. **
        "expression": "pk = :pk AND begins_with(sk, :sk)",
        "expressionValues" : {
            ":pk" : $util.dynamodb.toDynamoDBJson($ctx.args.customer),
            ":sk": $util.dynamodb.toDynamoDBJson($ctx.args.user)
        }
    }
}